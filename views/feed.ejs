<%- include('partials/header') -%>
<div class="container">
  <div class="row justify-content-center mt-5">
    <ul class="row list-unstyled">

    </ul>
  </div>
</div>

<h1>Rent Roll Example</h1>
<% if(tenants!=null) { %>
<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Landlord</th>
  </tr>

  <% tenants.forEach(function(item){ %>
  <tr>
    <td><%= item["firstName"]%></td>
    <td><%= item["lastName"] %></td>
    <td><%= item["landlordControl"]%></td>
  </tr>
  <% }) %>
</table>
<% } %>

<%- include('partials/footer') -%>